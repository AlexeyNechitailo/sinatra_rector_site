    / ================ contact section start =================
    %section.contact-section.section_padding
      .container
        .row
          .col-12
            %h2.contact-title Звʼязок
          .col-lg-8
            %form#contactForm.form-contact.contact_form{:action => "contact_process.php", :method => "post", :novalidate => "novalidate"}
              .row
                .col-12
                  .form-group
                    %textarea#message.form-control.w-100{:cols => "30", :name => "message", :onblur => "this.placeholder = 'Текст запитання'", :onfocus => "this.placeholder = ''", :placeholder => "Текст запитання", :rows => "9"}
                .col-sm-6
                  .form-group
                    %input#name.form-control{:name => "name", :onblur => "this.placeholder = 'Enter your name'", :onfocus => "this.placeholder = ''", :placeholder => "Імʼя", :type => "text"}/
                .col-sm-6
                  .form-group
                    %input#email.form-control{:name => "email", :onblur => "this.placeholder = 'Enter email address'", :onfocus => "this.placeholder = ''", :placeholder => "Електрона пошта", :type => "email"}/
                .col-12
                  .form-group
                    %input#subject.form-control{:name => "subject", :onblur => "this.placeholder = 'Enter Subject'", :onfocus => "this.placeholder = ''", :placeholder => "Тема", :type => "text"}/
              .form-group.mt-3
                %button.button.button-contactForm.btn_4.boxed-btn{:type => "submit"} Відправити
          .col-lg-4
            .media.contact-info
              %span.contact-info__icon
                %i.ti-home
              .media-body
